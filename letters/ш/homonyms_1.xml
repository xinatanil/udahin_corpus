<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:foo="http://whatever" xmlns:saxon="http://saxon.sf.net/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<card>
		<k>шайы</k>
		<pos>n</pos>
		<homonym>
			<origin>ир.</origin>
			<trn>канаус; шёлковая ткань.</trn>
		</homonym>
		<homonym>
			<origin>ир.</origin>
			<meta>чатк.</meta>
			<trn>коромысло весов.</trn>
		</homonym>
	</card>
	<card>
		<k>шараат</k>
		<pos>n</pos>
		<homonym>
			<sameas word="шарият"/>
		</homonym>
		<homonym>
			<sameas word="ишарат"/>
			<ex>
				<source>башын ийкеп, шараат кылып</source>
				<target>кивая головой и (тем самым) подавая знак.</target>
			</ex>
		</homonym>
	</card>
	<card>
		<k>шордо</k>
		<pos>v</pos>
		<homonym>
			<trn>(о скоте) лизать солонец.</trn>
		</homonym>
		<homonym>
			<trn>мучиться, испытывать муки.</trn>
		</homonym>
	</card>
	<card>
		<k>шыбоо</k>
		<pos>n</pos>
		<homonym>
			<trn>запах пота от ног;</trn>
			<ex>
				<source>бутунун шыбоосу бар</source>
				<target>у него от ног запах.</target>
			</ex>
		</homonym>
		<homonym>
			<actionNoun word="шыба" index="2"/>
		</homonym>
	</card>
	<card>
		<k>шыйпыр</k>
		<pos>n</pos>
		<homonym>
			<meta>разг.</meta>
			<sameas word="шифер"/>
		</homonym>
		<homonym>
			<meta>разг.</meta>
			<sameas word="сыйпыр"/>
		</homonym>
	</card>
	<card>
		<k>шыктаныш</k>
		<pos>ger</pos>
		<homonym>
			<actionNoun word="шыктан"/>
		</homonym>
		<homonym>
			<recv word="шыктан"/>
		</homonym>
	</card>
	<card>
		<k>шырык</k>
		<pos>n</pos>
		<homonym>
			<trn>строчка, прострачивание (когда материя складывается в два или несколько слоёв).</trn>
		</homonym>
		<homonym>
			<sameas word="шыргый"/>
		</homonym>
	</card>
	<card>
		<k>шырыкта</k>
		<pos>v</pos>
		<homonym>
			<trn>похрустывать, пощёлкивать (напр. суставами пальцев).</trn>
		</homonym>
		<homonym>
			<trn>окуривать (знахарский способ лечения).</trn>
		</homonym>
	</card>
</root>
